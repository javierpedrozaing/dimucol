<p id="notice"><%= notice %></p>

<div id="row">

<div class="col-xs-6 col-sm-2">
<%= render :template => "publications/_anuncioLeft" %>
</div>

<div class="col-xs-6 col-sm-6" style="border:1px solid;">
<h1>Publicidades de planes</h1>

<table>
  <thead>
    <tr>
      <th>Logo</th>
      <th>Nombrecomercial</th>
      <th>Email</th>
      <th>Direccion</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Web</th>
      <th>Experiencia</th>
      <th>Repertorio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicities.each do |publicity| %>
      <tr>
        <td><%= publicity.logo %></td>
        <td><%= publicity.nombreComercial %></td>
        <td><%= publicity.email %></td>
        <td><%= publicity.direccion %></td>
        <td><%= publicity.telefono %></td>
        <td><%= publicity.celular %></td>
        <td><%= publicity.web %></td>
        <td><%= publicity.experiencia %></td>
        <td><%= publicity.repertorio %></td>
        <td><%= link_to 'Show', publicity %></td>
        <td><%= link_to 'Edit', edit_publicity_path(publicity) %></td>
        <td><%= link_to 'Destroy', publicity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Publicity', new_publicity_path %>
</div>

<br>
<div class="col-xs-6 col-sm-2" >
<%= render :template => "publications/_anuncioRight" %>
</div>
<div class="clearfix visible-xs"></div>

<div class="col-xs-6 col-sm-2">
  <% if user_signed_in? %>
    <%= render :template => "publications/_columnChat" %>
  <% elsif %>
  <%= render :template => "publications/_anuncioExtra" %>
  <% end %>
</div>
</div>

  

